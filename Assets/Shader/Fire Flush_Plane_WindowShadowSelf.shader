          out_f.color.w = ((u_xlat0_d.x * u_xlat2) + _Opacity);
          u_xlat0_d.xy = TRANSFORM_TEX(in_f.texcoord.xy, _MainTex);
          u_xlat10_0.xyz = tex2D(_MainTex, u_xlat0_d.xy).xyz;
          out_f.color.xyz = u_xlat10_0.xyz;
          return out_f;
